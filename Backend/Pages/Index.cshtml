@model IEnumerable<Backend.Models.UserModel>
@{
    ViewBag.Title = "Users";
}
<h2>Films</h2>
<table class="table">
    <tr>
        <th>User ID</th>
        <th>Username</th>
        <th>Password</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Password)
            </td>
        </tr>
    }
</table>
